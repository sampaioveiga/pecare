<% provide(:title, t('.patients-index')) %>

<!-- aside column -->
<aside class="col-md-3">
  <div class="panel panel-default">
    <div class="panel-body">
      <%= render 'search-patients' %>  
    </div>
  </div>
</aside>

<div class="col-md-9">
  <div class="panel panel-default">
    <div class="panel-body">
      <ul class="patients">
        <%= render @patients %>
      </ul>
      <%= will_paginate %>
    </div>
  </div>
</div>
